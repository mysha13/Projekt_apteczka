<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="23dp"
        android:layout_toLeftOf="@id/imageView3"
        android:hint="Nazwa"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/autoCompleteTextView2"
        android:layout_marginEnd="20dp"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignEnd="@+id/autoCompleteTextView2"
        android:layout_alignStart="@+id/autoCompleteTextView2"
        android:layout_marginBottom="17dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/spinner3"
        android:layout_below="@+id/spinner3"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <ExpandableListView
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/editText2"
        android:layout_alignEnd="@+id/spinner3"
        android:layout_toEndOf="@+id/editText2" />

</RelativeLayout>